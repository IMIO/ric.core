<div id="ric-email-viewlet" class="ric-error"
     i18n:domain="ric"
     tal:condition="view/available">

     <tal:bcl tal:define="linksInfos view/linksInfos"
              tal:repeat="linkInfo linksInfos">
        <div tal:define="url linkInfo;
                         title python:linksInfos[linkInfo]">
             <span i18n:translate="">L'adresse email renseign&eacute;e dans votre description (<span i18n:name="title" tal:replace="title" />) n'est pas valide.</span>
             <a href=""
                class="person-link"
                tal:attributes="href string:${url}/edit#fieldsetlegend-contact_details"
                i18n:translate="">Cliquez ici pour la modifier.</a>
        </div>
     </tal:bcl>

</div>
